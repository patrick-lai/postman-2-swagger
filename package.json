{
  "name": "@iag-packages/postman-to-swagger",
  "version": "1.0.0",
  "description": "Postman collections to swagger",
  "main": "src/index.js",
  "scripts": {
    "compile": "npm run babel-compile",
    "compile:watch": "nodemon -x 'npm run compile'",
    "babel-compile": "babel src/ -d lib/ --ignore __tests__,*.spec.js",
    "commit": "git cz",
    "test": "jest",
    "eslint": "lint-staged",
    "git-add": "git add .",
    "coverage-shield": "coverage-shield"
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://s111258@chuck.auiag.corp/scm/digi/postman-to-swagger.git"
  },
  "keywords": [
    "postman",
    "api",
    "docs",
    "swagger",
    "converter"
  ],
  "author": "Patrick.p.lai@iag.com.au",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.1.2",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "jest": "^23.4.1",
    "jest-coverage-shield": "^1.1.0",
    "lint-staged": "^7.2.0",
    "pre-commit": "^1.2.2",
    "prettier": "^1.13.7"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 0
      }
    }
  },
  "lint-staged": {
    "*.js, *.jsx": [
      "eslint --fix"
    ]
  },
  "precommit": [
    "eslint",
    "test",
    "coverage-shield",
    "git-add"
  ],
  "peerDependencies": {
    "lodash": "^4.17.10"
  },
  "dependencies": {
    "postman-collection": "^3.2.1"
  }
}
